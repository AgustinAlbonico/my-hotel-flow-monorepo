{
  "name": "my-hotel-flow-monorepo",
  "private": true,
  "workspaces": [
    "apps/*"
  ],
  "scripts": {
    "bootstrap": "npm install",
    "build": "turbo run build",
    "dev:backend": "npm run -w @myhotelflow/backend dev",
    "dev:web": "npm run -w @myhotelflow/web dev",
    "dev": "turbo run dev --parallel",
    "start:backend": "npm run -w @myhotelflow/backend start",
    "start:web": "npm run -w @myhotelflow/web preview",
    "start": "turbo run start --parallel"
  },
  "devDependencies": {
    "concurrently": "^9.2.1",
    "turbo": "^2.6.1"
  }
}
